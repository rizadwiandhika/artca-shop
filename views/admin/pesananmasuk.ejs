<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->

    <title>ARTCA</title>

    <!-- Google font -->
    <link
      href="https://fonts.googleapis.com/css?family=Montserrat:400,500,700"
      rel="stylesheet"
    />

    <!-- Bootstrap -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
      crossorigin="anonymous"
    />

    <!-- Slick -->
    <link
      type="text/css"
      rel="stylesheet"
      href="/css/external/slick/slick.css"
    />
    <link
      type="text/css"
      rel="stylesheet"
      href="/css/external/slick/slick-theme.css"
    />

    <!-- nouislider -->
    <link
      type="text/css"
      rel="stylesheet"
      href="/css/external/nouislider/nouislider.min.css"
    />

    <!-- Font Awesome Icon -->
    <link
      rel="stylesheet"
      href="/css/external/font-awesome/font-awesome.min.css"
    />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.7.1/font/bootstrap-icons.css"
    />

    <!-- Custom stlylesheet -->
    <link type="text/css" rel="stylesheet" href="/css/shop/admin.css" />

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>
  <body>
    <!-- Navbar -->
    <nav class="navbar navbar-expand fixed-top navbar-light bg-light">
      <div class="container-fluid mx-5">
        <a class="navbar-brand" href="/shop/home">
          <img src="/img/Logo.svg" alt="Logo" height="50px" />
        </a>

        <form class="input-group mx-5"></form>

        <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
          <div class="navbar-nav">
            <a class="nav-link me-4" href="#">
              <img src="/img/Messages.svg" alt="Shopping Cart" height="25px" />
            </a>
            <a class="nav-link me-4" href="#">
              <img src="/img/Bell.svg" alt="Shopping Cart" height="25px" />
            </a>
          </div>
          <div id="profil-body">
            <a class="profil-body-budi" href="#">
              <img src="/img/profil-artca.svg" alt="profil" height="50px" />
            </a>
          </div>
        </div>
      </div>
    </nav>
    <!-- Navbar -->

    <div class="section">
      <div class="container">
        <div class="row">
          <!-- Product main img -->
          <div class="col-md-3 profil-site">
            <div id="profil-body">
              <a class="profil-body-budi" href="#">
                <img src="/img/profil-artca.svg" alt="profil" height="50px" />
                <h6>Artca</h6>
              </a>
            </div>
            <hr
              class="mb-3"
              style="width: 328px; margin-left: -20px; margin-top: 35px"
              ;
            />
            <div id="profil-chat">
              <div class="main">
                <a class="profil-chat-active" href="#">
                  <h6>Produk dan Kustomer</h6>
                </a>
                <a class="profil-chat-isi-1" href="/admin/daftarproduct">
                  <p>Daftar Produk</p>
                </a>
                <a class="profil-chat-isi-2" href="#">
                  <p>Pesanan Dikomplain</p>
                </a>
              </div>
              <hr
                class="mb-3"
                style="width: 328px; margin-left: -20px; margin-top: 5px"
                ;
              />
              <div class="tree">
                <a class="profil-chat-active" href="#"><h6>Transaksi</h6></a>
                <a class="profil-chat-isi-1" href="/admin/daftartransaksi">
                  <p>Daftar Transaksi</p>
                </a>
                <a class="profil-chat-isi-2" href="/admin/pesananmasuk">
                  <p>Pesanan Masuk</p>
                </a>
              </div>
            </div>
          </div>

          <div class="col-md-9">
            <div class="profil-main">
              <h6 class="judul">Daftar Transaksi</h6>
              <hr class="mb-3" style="width: 965px; margin-left: -20px" ; />
              <div class="search">
                <input
                  type="search"
                  class="form-control"
                  id="datatable-search-input"
                  placeholder="Search..."
                />
              </div>

              <div class="daftar-produk">
                <table class="table">
                  <thead>
                    <tr>
                      <th scope="col">Id</th>
                      <th scope="col">No. Resi</th>
                      <th scope="col">Harga</th>
                      <th scope="col">Tanggal</th>
                      <th scope="col">Detail</th>
                      <th class="aksi-head" scope="col">Aksi</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% for (let t of transactions) { %>
                    <tr>
                      <th scope="row"><%= t && t.id %></th>
                      <td>INV/20191241/XX/XII/134</td>
                      <td>Rp <%= t && t.totalPrice %></td>
                      <td><%= t && t.createdAt %></td>
                      <td>
                        <a
                          class="aksi"
                          href="/admin/bukti/transaksi/<%= t && t.id %>"
                        >
                          <i class="bi bi-search"></i>
                        </a>
                      </td>
                      <td class="aksi-in">
                        <form
                          id="form-setuju"
                          style="display: inline"
                          action="/admin/setuju/transaksi/<%= t && t.id %>"
                          method="post"
                        >
                          <a
                            id="setuju"
                            style="cursor: pointer; color: #2cda9d"
                          >
                            Setuju
                          </a>
                        </form>
                        /
                        <form
                          id="form-tolak"
                          style="display: inline"
                          action="/admin/tolak/transaksi/<%= t && t.id %>"
                          method="post"
                        >
                          <a
                            id="tolak"
                            style="cursor: pointer; color: #da2c2cc9"
                          >
                            Tolak
                          </a>
                        </form>
                      </td>
                    </tr>
                    <% } %>
                  </tbody>
                </table>
              </div>
              <nav class="halaman-no">
                <ul class="pagination pagination-sm justify-content-end">
                  <li class="page-item disabled">
                    <a class="page-link" href="#" tabindex="-1">Previous</a>
                  </li>
                  <li class="page-item"><a class="page-link" href="#">1</a></li>
                  <li class="page-item"><a class="page-link" href="#">2</a></li>
                  <li class="page-item"><a class="page-link" href="#">3</a></li>
                  <li class="page-item">
                    <a class="page-link" href="#">Next</a>
                  </li>
                </ul>
              </nav>
            </div>
          </div>
        </div>
      </div>
    </div>

    <footer class="text-center text-lg-start bg-light text-muted">
      <!-- Section: Links  -->
      <section class="">
        <div class="container text-center text-md-start">
          <!-- Grid row -->
          <div class="row">
            <!-- Grid column -->
            <div class="col-md-3 col-lg-4 col-xl-3 mx-auto mb-4">
              <!-- Content -->
              <h6 class="text-uppercase fw-bold mb-4">
                <img src="/img/Logo.svg" alt="Logo" height="50px" />
              </h6>
              <p>
                Artca merupakan perangkat lunak yang dikembangkan untuk
                mempermudah masyarakat dalam melakukan pembelian terhadap
                lukisan-lukisan
              </p>
            </div>
            <!-- Grid column -->

            <!-- Grid column -->
            <div class="col-md-2 col-lg-2 col-xl-2 mx-auto mb-4 bagian1-footer">
              <!-- Links -->
              <h6 class="text-uppercase fw-bold mb-4">ARTCA</h6>
              <p>
                <a href="#!" class="text-reset">Tentang ARTCA</a>
              </p>
              <p>
                <a href="#!" class="text-reset">Pusat Bantuan</a>
              </p>
              <p>
                <a href="#!" class="text-reset">Syarat dan Ketentuan</a>
              </p>
              <p>
                <a href="#!" class="text-reset">Kebijakan Privasi</a>
              </p>
            </div>
            <!-- Grid column -->

            <!-- Grid column -->
            <div class="col-md-3 col-lg-2 col-xl-2 mx-auto mb-4 bagian2-footer">
              <!-- Links -->
              <h6 class="text-uppercase fw-bold mb-4">Suarakan Idemu!</h6>
              <p>
                <a href="#!" class="text-reset">Lapor Bug</a>
              </p>
              <p>
                <a href="#!" class="text-reset">Feedback</a>
              </p>
            </div>
            <!-- Grid column -->
          </div>
          <!-- Section: Links  -->

          <!-- Copyright -->
          <div
            class="text-center p-4"
            style="background-color: rgba(0, 0, 0, 0.05)"
          >
            Â© 2021 Copyright: Artca Painting Shop/ PT. Aneka Usaha Indonesia
          </div>
          <!-- Copyright -->

          <!-- jQuery Plugins -->
          <script src="/js/jquery.min.js"></script>
          <script src="/js/bootstrap.min.js"></script>
          <script src="/js/slick.min.js"></script>
          <script src="/js/nouislider.min.js"></script>
          <script src="/js/jquery.zoom.min.js"></script>
          <script src="/js/main.js"></script>

          <script>
            const setuju = document.getElementById('setuju')
            const tolak = document.getElementById('tolak')

            setuju.addEventListener('click', function (e) {
              e.preventDefault()
              const form = document.getElementById('form-setuju')
              form.submit()
            })

            tolak.addEventListener('click', function (e) {
              e.preventDefault()
              const form = document.getElementById('form-tolak')
              form.submit()
            })
          </script>
        </div>
      </section>
    </footer>
  </body>
</html>
